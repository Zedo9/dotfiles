{
    "layer": "top",
    "modules-left": ["sway/window"],
    "modules-center": [],
    "modules-right": ["clock", "tray"],
    "sway/window": {
        "max-length": 50
    },
    "battery": {
        "format": "{capacity}% {icon}",
        "format-icons": ["", "", "", "", ""]
    },
    "clock": {
        "format-alt": "{:%a, %d. %b  %H:%M}"
    },
    "tray": {
        "icon-size": 21,
        "spacing": 10
    }
}
